<meta-model>

    <!--
     ~ This files contains additional global properties (HTML attributes) for
     ~ data-binding using GDB (see http.//gdb.thewebdev.guru/)
     -->

    <global-properties>
        <normal>
            <property name="data-bind-to" displayName="Data Binding Source" type="String"
                      tooltip="A single path to a location in the model"/>
            <property name="data-gdb-template" displayName="Data Template" type="String"
                      tooltip="A template composed of a mix of text and open and close delimiters containing a number"/>
        </normal>
    </global-properties>

</meta-model>